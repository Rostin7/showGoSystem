<script>
import "swiper/dist/css/swiper.min.css";
import ShareItem from "./shareItem";
export default {
  name: "detailSignUp",
  data() {
    return {
      value: 0,
      isDistributor: "",
      page: 1,
      size: 30,
      resellPrice: 0,
      noComment: false,
      isAll: true,
      dataAll: [],
      noticeList: [],
      activity_id: 1,
      mapSelect: false,
      type: 101,
      imgUrlSquare: "",
      imgUrlSignUp: require("../../assets/preview/marketIndex_bg.png"),
      img_url_bg: require("../../assets/preview/marketIndex_bg.png"),
      price: "",
      showPrice: "",
      endTime: 0,
      startTime: "",
      name: "",
      viewCount: 0,
      collectCount: 0,
      //attenFullText: '',
      //photoFullText: '',
      prizeFullText: "",
      photos: "", // 风采展示
      fullText: "",
      address: "",
      doorAdd: "",
      lat: "",
      lng: "",
      phoneNum: "",
      totalNum: "",
      alreadyNum: "",
      moduleId: 15,
      collect: "",
      userId: "",
      gotIt: "",
      content: "",
      isHide0: "",
      isHide1: "",
      isHide2: "",
      isHide3: "",
      nowIndex: 0,
      navItem: ["注意事项", "活动介绍", "活动奖品"],
      childData: [],
      childItem: [0],
      childTimeItem: [],
      dataTop: [],
      signUpRemind: false,
      signUpRemindType: 0,
      defaultTheme: "",
      swiperOption: {
        notNextTick: false,
        paginationClickable: true,
        autoHeight: true,
        observer: true,
        observeParents: true,
        loop: false,
        //touchAngle : 15,
        on: {
          slideChange: () => {
            this.nowIndex = this.$refs.mySwiper.swiper.realIndex;
          }
        }
      },
      defaultImgBg:
        'this.src="' + require("../../assets/preview/detail_bg.png") + '"',
      defaultImg:
        'this.src="' + require("../../assets/preview/signUp/default.png") + '"',
      nothing: false,
      navColor: "",
      isWeChat: false
    };
  },
  computed: {
    // 计算属性的 getter
    min_money() {
      // console.log(Math.min.apply(null, this.childItem))
      if (this.childItem.length != 0) {
        return Math.min.apply(null, this.childItem);
      } else {
        return null;
      }
    },
    max_money() {
      if (this.childItem.length != 0) {
        return Math.max.apply(null, this.childItem);
      } else {
        return null;
      }
    },
    minTime() {
      if (this.childTimeItem.length != 0) {
        return Math.min.apply(null, this.childTimeItem);
      } else {
        return null;
      }
    }
  },
  components: {
    Comment,
    ShareItem
  },

  methods: {
    tabClick(index) {
      this.$refs.mySwiper.swiper.slideTo(index, 300, true);
    },
    onShow(index) {
      switch (index) {
        case 0:
          return (this.isHide0 = false);
          break;
        case 1:
          return (this.isHide1 = false);
          break;
        case 2:
          return (this.isHide2 = false);
          break;
        case 3:
          return (this.isHide3 = false);
          break;
        default:
          return;
      }
    },
    getDataList() {
      let url = this.apiList.shop.enterName.detail
      this.$http({
        url: this.$http.adornUrl(url),
        method: "post",
        data: this.$http.adornData({ id: this.activity_id },url, true)
      }).then(({ data }) => {
        if (data.result) {
          this.imgUrlSignUp = data.data.imgUrl;
          this.img_url_bg = data.data.imgUrl;
          this.imgUrlSquare = data.data.imgUrlSquare;
          this.price = data.data.price / 100;
          this.showPrice = data.data.showPrice / 100;
          this.endTime = data.data.endTime;
          this.startTime = data.data.startTime;
          this.address = data.data.address;
          this.doorAdd = data.data.doorAdd;
          this.name = data.data.name;
          this.viewCount = data.data.viewCount;
          this.collectCount = data.data.collectCount;
          this.alreadyNum = data.data.alreadyNum;
          this.totalNum = data.data.totalNum;
          this.phoneNum = data.data.phoneNum;
          this.moduleId = data.data.moduleId;
          this.type = data.data.type;
          this.collect = data.data.collect;
          this.lng = data.data.lng;
          this.lat = data.data.lat;
          this.gotIt = data.data.gotIt;
          this.dataAll = data.data
          this.noticeList = data.data.attention
          if (!this.defaultTheme) {
            this.defaultTheme = data.data.defaultTheme;
          }
          if (!this.defaultTheme) {
            this.navColor = "#00B0FF";
          } else if (this.defaultTheme === 19) {
            this.navColor = "#D2D1F6";
          } else if (this.defaultTheme === 13) {
            this.navColor = "#F091F5";
          } else if (this.defaultTheme === 5) {
            this.navColor = "#ECF2D8";
          } else if (this.defaultTheme === 8) {
            this.navColor = "#ECF4F7";
          } else if (this.defaultTheme === 25 || this.defaultTheme > 46) {
            this.navColor = "#ffffff";
          } else if (this.defaultTheme === 31) {
            this.navColor = "#D9E8D2";
          } else if (this.defaultTheme === 37) {
            this.navColor = "#33CEFE";
          } else if (this.defaultTheme === 41) {
            this.navColor = "#8043EE";
          }
          this.photos = data.data.photos ? JSON.parse(data.data.photos) : []
          this.prizeFullText = data.data.prizeFullText
            ? Base64.decode(data.data.prizeFullText)
            : "";
          this.fullText = data.data.fullText
            ? Base64.decode(data.data.fullText)
            : ""
          this.$nextTick(() => {
            let h2 = document.querySelector("#fullText1").offsetHeight;
            let h3 = document.querySelector("#fullText2").offsetHeight;
            if (h2 >= 280) {
              this.isHide1 = true;
            } else {
              this.isHide1 = false;
            }
            if (h3 >= 280) {
              this.isHide2 = true;
            } else {
              this.isHide2 = false;
            }
          });
        } else if (data.msg === "不存在") {
          this.nothing = true;
        }
      })
    }
  
  },
  created() {
    this.defaultTheme = 19
  }
};
</script>
